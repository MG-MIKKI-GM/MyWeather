<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="375" d:DesignHeight="800"
             x:Class="MyWeather.Views.MainPageWindows"
             xmlns:sun="clr-namespace:MyWeather.Views" Name="mpw">

	<UserControl.Styles>
		<Style Selector="Image:pointerover">
			<Setter Property="Opacity" Value="0.5"/>
		</Style>
		<Style Selector="TextBlock">
			<Setter Property="FontFamily" Value="Ubuntu Condensed"/>
		</Style>
	</UserControl.Styles>

	<Grid Background="White">
		<Grid Name="MobileVersionPanel" IsVisible="False">
			<Grid.RowDefinitions>
				<RowDefinition Height="120"/>
				<RowDefinition Height="*"/>
			</Grid.RowDefinitions>
			<StackPanel IsVisible="False" Name="MobileBackBtn" ZIndex="1" Margin="65,30,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Orientation="Horizontal">
				<Grid Margin="0,6,15,0">
					<Grid.Styles>
						<Style Selector="Border:pointerover">
							<Setter Property="Opacity" Value="0.5"/>
						</Style>
					</Grid.Styles>
					<Border Width="10" Height="3" CornerRadius="2" BorderThickness="2" BorderBrush="#646464">
						<Border.RenderTransform>
							<RotateTransform Angle="45"/>
						</Border.RenderTransform>
					</Border>
					<Border Width="10" Height="3" Margin="0,0,0,10" CornerRadius="2" BorderThickness="2" BorderBrush="#646464">
						<Border.RenderTransform>
							<RotateTransform Angle="-45"/>
						</Border.RenderTransform>
					</Border>
				</Grid>
				<TextBlock Text="Прогноз на сегодня" Foreground="#646464"/>
			</StackPanel>
			<Grid Grid.Row="0" Margin="30,60,30,0" ColumnDefinitions="*, *">
				<StackPanel Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Left">
					<TextBlock VerticalAlignment="Center" Text="{Binding WeatherCity.City, ElementName=mpw}" FontSize="18" FontWeight="Medium"/>
					<TextBlock VerticalAlignment="Center" Text="Текущее место" FontSize="10" FontWeight="Light"/>
					<TextBlock Text="{Binding WeatherCity.Favourites, ElementName=mpw}" Name="MobileFavouritesText" FontSize="10" Foreground="#646464">
						<TextBlock.Styles>
							<Style Selector="TextBlock:pointerover">
								<Setter Property="Opacity" Value="0.5"/>
							</Style>
						</TextBlock.Styles>
					</TextBlock>
				</StackPanel>
				<StackPanel Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Orientation="Horizontal">
					<Image Name="SearchImageM" Height="21" Source="/Assets/Locations.png" Margin="0,0,0,0"/>
					<Image Name="SettingsImageM" Height="21" Margin="32,0,0,0" Source="/Assets/settings.png" Stretch="Uniform"/>
					<Image Name="LoveImageM" Height="21" Margin="32,0,0,0" Source="/Assets/love.png" Stretch="Uniform"/>
				</StackPanel>
			</Grid>
			<ScrollViewer VerticalScrollBarVisibility="Hidden" Name="Mobile7Panel" IsVisible="False" Grid.Row="1">
				<StackPanel Margin="0,50,0,0">
					<TextBlock Text="ПРОГНОЗ НА СЕГОДНЯ" HorizontalAlignment="Center" FontSize="24" FontWeight="Medium"/>
					<Grid Margin="20,30,20,0" ColumnDefinitions="* * * * * * * *">
						<Grid.Styles>
							<Style Selector="TextBlock">
								<Setter Property="FontSize" Value="12"/>
								<Setter Property="Foreground" Value="#616161"/>
							</Style>
							<Style Selector="Image">
								<Setter Property="Height" Value="24"/>
								<Setter Property="Margin" Value="0,10,0,0"/>
							</Style>
							<Style Selector="StackPanel">
								<Setter Property="HorizontalAlignment" Value="Center"/>
							</Style>
						</Grid.Styles>
						<StackPanel Grid.Column="0" >
							<TextBlock Text="{Binding WeatherCity.Times[0], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[0], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="1" >
							<TextBlock Text="{Binding WeatherCity.Times[1], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[1], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="2" >
							<TextBlock Text="{Binding WeatherCity.Times[2], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[2], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="3" >
							<TextBlock Text="{Binding WeatherCity.Times[3], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[3], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="4" >
							<TextBlock Text="{Binding WeatherCity.Times[4], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[4], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="5" >
							<TextBlock Text="{Binding WeatherCity.Times[5], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[5], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="6" >
							<TextBlock Text="{Binding WeatherCity.Times[6], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[6], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
						<StackPanel Grid.Column="7" >
							<TextBlock Text="{Binding WeatherCity.Times[7], ElementName=mpw}"/>
							<Image Source="{Binding WeatherCity.Icons[7], ElementName=mpw}"   Stretch="Uniform"/>
						</StackPanel>
					</Grid>
					<StackPanel Margin="20,30,0,0">
						<StackPanel.Styles>
							<Style Selector="TextBlock">
								<Setter Property="FontSize" Value="12"/>
							</Style>
						</StackPanel.Styles>
						<TextBlock Margin="0,10,0,0" Foreground="#646464" FontSize="18" Text="Ветер"/>
						<Grid ColumnDefinitions="*,*,*,*">
							<StackPanel Grid.Column="0" HorizontalAlignment="Left">
								<TextBlock Text="Утро" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.MorningSpeeds[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.SpeedText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
							<StackPanel Grid.Column="1" HorizontalAlignment="Left">
								<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.DaySpeeds[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.SpeedText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
							<StackPanel Grid.Column="2" HorizontalAlignment="Left">
								<TextBlock Text="Вечер" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.EveningSpeeds[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.SpeedText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
							<StackPanel Grid.Column="3" HorizontalAlignment="Left">
								<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.NightSpeeds[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.SpeedText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
						</Grid>
						<TextBlock Margin="0,25,0,0" Foreground="#646464" FontSize="18" Text="Влажность"/>
						<Grid ColumnDefinitions="*,*,*,*">
							<StackPanel Grid.Column="0" HorizontalAlignment="Left">
								<TextBlock Text="Утро" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.MorningHumidities[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.HumidityText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
							<StackPanel Grid.Column="1" HorizontalAlignment="Left">
								<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.DayHumidities[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.HumidityText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
							<StackPanel Grid.Column="2" HorizontalAlignment="Left">
								<TextBlock Text="Вечер" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.EveningHumidities[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.HumidityText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
							<StackPanel Grid.Column="3" HorizontalAlignment="Left">
								<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
								<StackPanel Orientation="Horizontal">
									<TextBlock Text="{Binding WeatherCity.NightHumidities[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
									<TextBlock Text="{Binding WeatherCity.HumidityText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								</StackPanel>
							</StackPanel>
						</Grid>
						<TextBlock Margin="0,25,0,0" Foreground="#646464" FontSize="18" Text="Давление"/>
						<Grid ColumnDefinitions="*,*,*,*">
							<StackPanel Grid.Column="0" HorizontalAlignment="Left">
								<TextBlock Text="Утро" HorizontalAlignment="Center" Foreground="#646464"/>
								<TextBlock Text="{Binding WeatherCity.MorningPressures[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								<TextBlock Text="{Binding WeatherCity.PressureText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="#646464" FontSize="12" FontWeight="Medium"/>
							</StackPanel>
							<StackPanel Grid.Column="1" HorizontalAlignment="Left">
								<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
								<TextBlock Text="{Binding WeatherCity.DayPressures[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								<TextBlock Text="{Binding WeatherCity.PressureText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="#646464" FontSize="12" FontWeight="Medium"/>
							</StackPanel>
							<StackPanel Grid.Column="2" HorizontalAlignment="Left">
								<TextBlock Text="Вечер" HorizontalAlignment="Center" Foreground="#646464"/>
								<TextBlock Text="{Binding WeatherCity.EveningPressures[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								<TextBlock Text="{Binding WeatherCity.PressureText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="#646464" FontSize="12" FontWeight="Medium"/>
							</StackPanel>
							<StackPanel Grid.Column="3" HorizontalAlignment="Left">
								<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
								<TextBlock Text="{Binding WeatherCity.NightPressures[0], ElementName=mpw}" HorizontalAlignment="Center" Foreground="Black" FontSize="18" FontWeight="Medium"/>
								<TextBlock Text="{Binding WeatherCity.PressureText, ElementName=mpw}" HorizontalAlignment="Center" Foreground="#646464" FontSize="12" FontWeight="Medium"/>
							</StackPanel>
						</Grid>
					</StackPanel>
					<StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Margin="0,35,0,0">
						<StackPanel Margin="0,0,50,0" Orientation="Horizontal">
							<Image Height="21" Margin="0,0,5,0" Source="/Assets/sunrise.png"/>
							<TextBlock FontSize="18" Text="{Binding WeatherCity.Sunrises[0], ElementName=mpw}" Foreground="#646464"/>
						</StackPanel>
						<StackPanel Margin="50,0,0,0" Orientation="Horizontal">
							<Image Height="21" Margin="0,0,5,0" Source="/Assets/sunset.png"/>
							<TextBlock FontSize="18" Text="{Binding WeatherCity.Sunsets[0], ElementName=mpw}" Foreground="#646464"/>
						</StackPanel>
					</StackPanel>
					<StackPanel HorizontalAlignment="Left" Margin="20,0,0,0">
						<TextBlock Margin="10,30,0,0" Text="Магнитное поле:  " FontSize="18" Foreground="#646464">
							<Run Text="спокойное" Foreground="Black" FontSize="16"></Run>
						</TextBlock>
						<TextBlock Margin="10,5,0,10" Text="УФ-индекс:  " FontSize="18" Foreground="#646464">
							<Run Text="{Binding WeatherCity.UFIndex[0], ElementName=mpw}" FontSize="16" Foreground="Black"></Run>
						</TextBlock>
					</StackPanel>
				</StackPanel>
			</ScrollViewer>
			
			<ScrollViewer Name="MobileMainPanel" IsVisible="True" VerticalScrollBarVisibility="Hidden" Grid.Row="1" >
				<StackPanel Margin="0,60,0,0">
					<StackPanel HorizontalAlignment="Center">
						<StackPanel.Styles>
							<Style Selector="TextBlock">
								<Setter Property="FontWeight" Value="Medium"/>
							</Style>
						</StackPanel.Styles>
						<StackPanel>
							<TextBlock Text="{Binding WeatherCity.Weeks[0], ElementName=mpw}" FontSize="20" Foreground="#646464" Margin="0,0,0,0" HorizontalAlignment="Center"/>
							<StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Margin="0,10,0,0">
								<TextBlock Text="{Binding WeatherCity.Days[0], ElementName=mpw}" VerticalAlignment="Center" FontSize="18" Foreground="#646464" HorizontalAlignment="Center"/>
								<TextBlock Text="{Binding WeatherCity.Months[0], ElementName=mpw}" FontSize="18" Margin="2,0,0,0" Foreground="#646464" VerticalAlignment="Center"/>
							</StackPanel>
							<StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
								<TextBlock FontSize="96" Foreground="Black" Text="{Binding WeatherCity.Temps[0], ElementName=mpw}" />
								<TextBlock FontSize="36" Foreground="Black" Text="{Binding WeatherCity.TempText, ElementName=mpw}" Margin="0,0,0,15" VerticalAlignment="Bottom"/>
							</StackPanel>
							<StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
								<Grid Margin="0,2,0,0">
									<Border Width="1" Height="13" CornerRadius="10" Background="#646464" BorderThickness="0"/>
									<Border Width="6" Height="1" CornerRadius="10" Margin="0,9,3.5,0" Background="#646464" BorderThickness="0">
										<Border.RenderTransform>
											<RotateTransform Angle="45"/>
										</Border.RenderTransform>
									</Border>
									<Border Width="6" Height="1" CornerRadius="10" Margin="3.5,9,0,0" Background="#646464" BorderThickness="0">
										<Border.RenderTransform>
											<RotateTransform Angle="-45"/>
										</Border.RenderTransform>
									</Border>
								</Grid>
								<TextBlock Foreground="#646464" FontSize="18">
									<Run Text="{Binding WeatherCity.MinTemps[0], ElementName=mpw}"/>
									<Run Text="{Binding WeatherCity.TempText, ElementName=mpw}"/>
								</TextBlock>
								<Grid Margin="25,2,0,0">
									<Border Width="1" Height="13" CornerRadius="10" Background="#646464" BorderThickness="0"/>
									<Border Width="6" Height="1" CornerRadius="10" Margin="0,0,3.5,9" Background="#646464" BorderThickness="0">
										<Border.RenderTransform>
											<RotateTransform Angle="135"/>
										</Border.RenderTransform>
									</Border>
									<Border Width="6" Height="1" CornerRadius="10" Margin="3.5,0,0,9" Background="#646464" BorderThickness="0">
										<Border.RenderTransform>
											<RotateTransform Angle="-135"/>
										</Border.RenderTransform>
									</Border>
								</Grid>
								<TextBlock Foreground="#646464" FontSize="18">
									<Run Text="{Binding WeatherCity.MaxTemps[0], ElementName=mpw}"/>
									<Run Text="{Binding WeatherCity.TempText, ElementName=mpw}"/>
								</TextBlock>
							</StackPanel>
							<StackPanel HorizontalAlignment="Center" Margin="0,40,0,0">
								<Image Height="120" Source="{Binding WeatherCity.Dayicons[0], ElementName=mpw}"/>
								<TextBlock HorizontalAlignment="Center" Text="{Binding WeatherCity.Condition[0], ElementName=mpw}" Foreground="#646464" FontSize="18" Margin="0,10,0,0"/>
								<TextBlock Text="Подробнее" Name="WeekText" FontSize="10" Foreground="#646464" HorizontalAlignment="Center" Margin="0,10,0,0">
									<TextBlock.Styles>
										<Style Selector="TextBlock:pointerover">
											<Setter Property="Opacity" Value="0.5"/>
										</Style>
									</TextBlock.Styles>
								</TextBlock>
							</StackPanel>
						</StackPanel>
					</StackPanel>
					<StackPanel HorizontalAlignment="Left" Margin="30,50,0,0">
						<TextBlock Text="ПРОГНОЗ НА 7 ДНЕЙ" FontSize="24" FontWeight="Medium"/>
					</StackPanel>
					<StackPanel Margin="30,20">
						<Border BoxShadow="0 4 4 0 #bfbfbf" Margin="0,0,0,10" Height="50" CornerRadius="15">
							<Grid VerticalAlignment="Center" ColumnDefinitions="1.5* * *">
								<StackPanel Margin="10,0,0,0" Grid.Column="0">
									<TextBlock Foreground="#646464">
										<Run Text="{Binding WeatherCity.Days[0], ElementName=mpw}"/>
										<Run Text="{Binding WeatherCity.Months[0], ElementName=mpw}"/>
									</TextBlock>
									<TextBlock Text="сегодня" FontWeight="Medium" FontSize="18" Foreground="Black"/>
								</StackPanel>
								<StackPanel Margin="0,0,20,0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right">
									<Image HorizontalAlignment="Center" Grid.Column="0" Height="24" Source="{Binding WeatherCity.Dayicons[0], ElementName=mpw}"/>
								</StackPanel>
								<Grid ColumnDefinitions="* *" Grid.Column="2">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="0">
										<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MinTemps[0], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="1">
										<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MaxTemps[0], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
								</Grid>
							</Grid>
						</Border>
						<Border BoxShadow="0 4 4 0 #bfbfbf" Margin="0,0,0,10"  Height="50" CornerRadius="15">
							<Grid VerticalAlignment="Center" ColumnDefinitions="1.5* * *">
								<StackPanel Margin="10,0,0,0" Grid.Column="0">
									<TextBlock Foreground="#646464">
										<Run Text="{Binding WeatherCity.Days[1], ElementName=mpw}"/>
										<Run Text="{Binding WeatherCity.Months[1], ElementName=mpw}"/>
									</TextBlock>
									<TextBlock Text="{Binding WeatherCity.Weeks[1], ElementName=mpw}" FontWeight="Medium" FontSize="18" Foreground="Black"/>
								</StackPanel>
								<StackPanel Margin="0,0,20,0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right">
									<Image HorizontalAlignment="Center" Grid.Column="0" Height="24" Source="{Binding WeatherCity.Dayicons[1], ElementName=mpw}"/>
								</StackPanel>
								<Grid ColumnDefinitions="* *" Grid.Column="2">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="0">
										<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MinTemps[1], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="1">
										<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MaxTemps[1], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
								</Grid>
							</Grid>
						</Border>
						<Border BoxShadow="0 4 4 0 #bfbfbf" Margin="0,0,0,10"  Height="50" CornerRadius="15">
							<Grid VerticalAlignment="Center" ColumnDefinitions="1.5* * *">
								<StackPanel Margin="10,0,0,0" Grid.Column="0">
									<TextBlock Foreground="#646464">
										<Run Text="{Binding WeatherCity.Days[2], ElementName=mpw}"/>
										<Run Text="{Binding WeatherCity.Months[2], ElementName=mpw}"/>
									</TextBlock>
									<TextBlock Text="{Binding WeatherCity.Weeks[2], ElementName=mpw}" FontWeight="Medium" FontSize="18" Foreground="Black"/>
								</StackPanel>
								<StackPanel Margin="0,0,20,0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right">
									<Image HorizontalAlignment="Center" Grid.Column="0" Height="24" Source="{Binding WeatherCity.Dayicons[2], ElementName=mpw}"/>
								</StackPanel>
								<Grid ColumnDefinitions="* *" Grid.Column="2">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="0">
										<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MinTemps[2], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="1">
										<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MaxTemps[2], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
								</Grid>
							</Grid>
						</Border>
						<Border BoxShadow="0 4 4 0 #bfbfbf" Margin="0,0,0,10"  Height="50" CornerRadius="15">
							<Grid VerticalAlignment="Center" ColumnDefinitions="1.5* * *">
								<StackPanel Margin="10,0,0,0" Grid.Column="0">
									<TextBlock Foreground="#646464">
										<Run Text="{Binding WeatherCity.Days[3], ElementName=mpw}"/>
										<Run Text="{Binding WeatherCity.Months[3], ElementName=mpw}"/>
									</TextBlock>
									<TextBlock Text="{Binding WeatherCity.Weeks[3], ElementName=mpw}" FontWeight="Medium" FontSize="18" Foreground="Black"/>
								</StackPanel>
								<StackPanel Margin="0,0,20,0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right">
									<Image HorizontalAlignment="Center" Grid.Column="0" Height="24" Source="{Binding WeatherCity.Dayicons[3], ElementName=mpw}"/>
								</StackPanel>
								<Grid ColumnDefinitions="* *" Grid.Column="2">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="0">
										<TextBlock Text="День" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MinTemps[3], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="1">
										<TextBlock Text="Ночь" HorizontalAlignment="Center" Foreground="#646464"/>
										<TextBlock HorizontalAlignment="Center" FontSize="18">
											<Run Text="{Binding WeatherCity.MaxTemps[3], ElementName=mpw}"/>
											<Run Text="°"/>
										</TextBlock>
									</StackPanel>
								</Grid>
